{
  "name": "Client",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "emailVerificationRequired": true
  },
  "comments": "Guests are never explicitly logged in or out.  Credentials stored on client.",
  "properties": {
    "created": {
      "type": "Date",
      "required": false,
      "default": "1900-01-01 00:00:00"
    },
    "username": {
      "type": "string",
      "required": true,
      "length": 40
    },
    "membershipExpiry": {
      "type": "Date",
      "required": false,
      "default": "1900-01-01 00:00:00"
    }
  },
  "validations": [],
  "relations": {
    "students": {
      "type": "hasMany",
      "model": "Client",
      "foreignKey": ""
    },
    "peers": {
      "type": "hasMany",
      "model": "Client",
      "foreignKey": ""
    },
    "userSettings": {
      "type": "hasOne",
      "model": "UserSettings",
      "foreignKey": ""
    },
    "exerciseSets": {
      "type": "hasMany",
      "model": "ExerciseSet",
      "foreignKey": "",
      "through": "ClientExerciseSet"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "administrator",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "guest",
      "permission": "DENY",
      "property": "logout",
      "model": "Client"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "guest",
      "permission": "DENY",
      "property": "login",
      "model": "Client"
    }
  ],
  "methods": {}
}
